<?xml version="1.0" encoding="ISO-8859-1"?>
<template type="ProjTempl" version="1.0" supplier="Eclipse.org"
	revision="1.0" author="Liviu Ionescu"
	copyright="Copyright (c) 2014 Liviu Ionescu. All rights reserved. This \
program and the accompanying materials are made available under the terms \
of the Eclipse Public License v1.0 which accompanies this distribution, \
and is available athttp://www.eclipse.org/legal/epl-v10.html"
	id="CM_Project" label="%CM.template.label" description="%CM.template.description"
	help="help.html">

	<property-group id="chip" label="%CM.group.target.label"
		description="%CM.group.target.description" type="PAGES-ONLY" help="help.html">

		<property id="CMflashSize" label="%CM.flashSize.label"
			description="%CM.flashSize.description" type="input" pattern="[0-9]*"
			default="%CM.flashSize.default" hidden="false" persist="true" />
		<property id="CMramSize" label="%CM.ramSize.label"
			description="%CM.ramSize.description" type="input" pattern="[0-9]*"
			default="%CM.ramSize.default" hidden="false" persist="true" />

		<property id="CMxtalValue" label="%CM.xtalValue.label"
			description="%CM.xtalValue.description" type="input" pattern="[0-9]*"
			default="%CM.xtalValue.default" hidden="false" persist="true" />

		<property id="CMsyscalls" label="%CM.syscalls.label"
			description="%CM.syscalls.description" type="select" default="retarget"
			hidden="false" persist="true">
			<item value="none"
				label="Standalone (no printf() or other POSIX system calls)" />
			<item value="retarget"
				label="POSIX (system calls redirected to application code)" />
			<item value="semihosting" label="Semihosting (POSIX system calls via host)" />
		</property>

		<property id="CMmostWarnings" label="%CM.mostWarnings.label"
			description="%CM.mostWarnings.description" type="boolean" default="true"
			hidden="false" persist="true" />

		<property id="CMuseNano" label="%CM.useNano.label"
			description="%CM.useNano.description" type="boolean" default="true"
			hidden="false" persist="true" />

	</property-group>

	<property-group id="folders" label="%CM.group.folders.label"
		description="%CM.group.folders.description" type="PAGES-ONLY" help="help.html">
		<property id="includeDir" label="%CM.includeDir.label"
			description="%CM.includeDir.description" type="input"
			default="%CM.includeDir.default" pattern="[a-zA-Z0-9]+" mandatory="true"
			persist="true" />
		<property id="sourceDir" label="%CM.sourceDir.label"
			description="%CM.sourceDir.description" type="input" default="%CM.sourceDir.default"
			pattern="[a-zA-Z0-9]+" mandatory="true" persist="true" />
		<property id="sysDir" label="%CM.sysDir.label" description="%CM.sysDir.description"
			type="input" default="%CM.sysDir.default" pattern="[a-zA-Z0-9]+"
			mandatory="true" persist="true" />
		<property id="cmsisDir" label="%CM.cmsisDir.label"
			description="%CM.cmsisDir.description" type="input" default="%CM.cmsisDir.default"
			pattern="[a-zA-Z0-9]+" mandatory="true" persist="true" />
		<property id="newlibDir" label="%CM.newlibDir.label"
			description="%CM.newlibDir.description" type="input" default="%CM.newlibDir.default"
			pattern="[a-zA-Z0-9]+" mandatory="true" persist="true" />
		<property id="ldscriptsDir" label="%CM.ldscriptsDir.label"
			description="%CM.ldscriptsDir.description" type="input"
			default="%CM.ldscriptsDir.default" pattern="[a-zA-Z0-9]+" mandatory="true"
			persist="true" />
		<property id="deviceName" label="%CM.deviceName.label"
			description="%CM.deviceName.description" type="input"
			default="%CM.deviceName.default" pattern="[a-zA-Z0-9]+" mandatory="true"
			persist="true" />

		<property id="fileExtension" label="Source file extension"
			description="Will be set to cpp for ccnature" mandatory="true"
			hidden="true" default="c" persist="false" />

		<property id="commonDir" label="common files" description="Will be set by default"
			mandatory="true" hidden="true" default="../common" persist="false" />

	</property-group>

	<!-- Note: the redundant 'condition="1==1"' is a workaround to control the 
		evaluation order. Without it, processes are evaluated after all ifs. -->

	<if condition="1==1">
		<process type="org.eclipse.cdt.managedbuilder.core.NewManagedProject">
			<simple name="name" value="$(projectName)" />
			<simple name="artifactExtension" value="elf" />
			<simple name="isCProject" value="true" />
		</process>

		<process type="ilg.gnuarmeclipse.templates.core.SetPropertyIfHasNature">
			<simple name="projectName" value="$(projectName)" />
			<simple name="natureString" value="org.eclipse.cdt.core.ccnature" />
			<simple name="propertyName" value="fileExtension" />
			<simple name="propertyValue" value="cpp" />
		</process>

		<!-- include -->
		<process type="org.eclipse.cdt.core.CreateFolder">
			<simple name="projectName" value="$(projectName)" />
			<simple name="path" value="$(includeDir)" />
		</process>

		<!-- add to include paths -->
		<process
			type="org.eclipse.cdt.managedbuilder.core.AppendToMBSStringListOptionValues">
			<simple name="projectName" value="$(projectName)" />
			<complex-array name="resourcePaths">
				<element>
					<simple name="id"
						value="ilg.gnuarmeclipse.managedbuild.cross.option.*.include.paths" />
					<simple-array name="values">
						<element value="&quot;../$(includeDir)&quot;" />
					</simple-array>
					<simple name="path" value="" />
				</element>
			</complex-array>
		</process>

		<process type="org.eclipse.cdt.core.AddFiles">
			<simple name="projectName" value="$(projectName)" />
			<complex-array name="files">
				<element>
					<simple name="source" value="$(commonDir)/include/Timer-$(fileExtension).h" />
					<simple name="target" value="$(includeDir)/Timer.h" />
					<simple name="replaceable" value="true" />
				</element>
			</complex-array>
		</process>
	</if>

	<if condition="1==1">
		<!-- src -->
		<process type="org.eclipse.cdt.core.CreateSourceFolder">
			<simple name="projectName" value="$(projectName)" />
			<simple name="path" value="$(sourceDir)" />
		</process>

		<process type="org.eclipse.cdt.core.AddFiles">
			<simple name="projectName" value="$(projectName)" />
			<complex-array name="files">
				<element>
					<simple name="source" value="$(commonDir)/src/Timer.$(fileExtension)" />
					<simple name="target" value="$(sourceDir)/Timer.$(fileExtension)" />
					<simple name="replaceable" value="true" />
				</element>
			</complex-array>
		</process>
	</if>

	<!-- the main.[c|cpp] file -->
	<if condition="1==1">
		<process type="org.eclipse.cdt.core.AddFiles">
			<simple name="projectName" value="$(projectName)" />
			<complex-array name="files">
				<element>
					<simple name="source" value="src/main.$(fileExtension)" />
					<simple name="target" value="$(sourceDir)/main.$(fileExtension)" />
					<simple name="replaceable" value="true" />
				</element>
			</complex-array>
		</process>
	</if>

	<if condition="1==1">
		<!-- ldscripts -->
		<process type="org.eclipse.cdt.core.CreateFolder">
			<simple name="projectName" value="$(projectName)" />
			<simple name="path" value="$(ldscriptsDir)" />
		</process>

		<process type="org.eclipse.cdt.core.AddFiles">
			<simple name="projectName" value="$(projectName)" />
			<complex-array name="files">
				<element>
					<simple name="source" value="ldscripts/mem.ld" />
					<simple name="target" value="$(ldscriptsDir)/mem.ld" />
					<simple name="replaceable" value="true" />
				</element>
				<element>
					<simple name="source" value="$(commonDir)/ldscripts/libs.ld" />
					<simple name="target" value="$(ldscriptsDir)/libs.ld" />
					<simple name="replaceable" value="true" />
				</element>
				<element>
					<simple name="source" value="$(commonDir)/ldscripts/sections.ld" />
					<simple name="target" value="$(ldscriptsDir)/sections.ld" />
					<simple name="replaceable" value="true" />
				</element>
			</complex-array>
		</process>
	</if>

	<if condition="1==1">
		<!-- system as src -->
		<process type="org.eclipse.cdt.core.CreateSourceFolder">
			<simple name="projectName" value="$(projectName)" />
			<simple name="path" value="$(sysDir)" />
		</process>
	</if>

	<if condition="1==1">
		<!-- system/include/cmsis -->
		<process type="org.eclipse.cdt.core.CreateFolder">
			<simple name="projectName" value="$(projectName)" />
			<simple name="path" value="$(sysDir)/$(includeDir)/$(cmsisDir)" />
		</process>

		<!-- add to include paths -->
		<process
			type="org.eclipse.cdt.managedbuilder.core.AppendToMBSStringListOptionValues">
			<simple name="projectName" value="$(projectName)" />
			<complex-array name="resourcePaths">
				<element>
					<simple name="id"
						value="ilg.gnuarmeclipse.managedbuild.cross.option.*.include.paths" />
					<simple-array name="values">
						<element value="&quot;../$(sysDir)/$(includeDir)&quot;" />
					</simple-array>
					<simple name="path" value="" />
				</element>
				<element>
					<simple name="id"
						value="ilg.gnuarmeclipse.managedbuild.cross.option.*.include.paths" />
					<simple-array name="values">
						<element value="&quot;../$(sysDir)/$(includeDir)/$(cmsisDir)&quot;" />
					</simple-array>
					<simple name="path" value="" />
				</element>
			</complex-array>
		</process>
	</if>

	<if condition="1==1">
		<process type="org.eclipse.cdt.core.AddFiles">
			<simple name="projectName" value="$(projectName)" />
			<complex-array name="files">
				<element>
					<simple name="source" value="$(commonDir)/system/include/cmsis/README.txt" />
					<simple name="target" value="$(sysDir)/$(includeDir)/$(cmsisDir)/README.txt" />
					<simple name="replaceable" value="true" />
				</element>
			</complex-array>
		</process>

		<process type="org.eclipse.cdt.core.AddFiles">
			<simple name="projectName" value="$(projectName)" />
			<complex-array name="files">
				<element>
					<simple name="source"
						value="$(commonDir)/system/include/cmsis/arm_common_tables.h" />
					<simple name="target"
						value="$(sysDir)/$(includeDir)/$(cmsisDir)/arm_common_tables.h" />
					<simple name="replaceable" value="true" />
				</element>
				<element>
					<simple name="source"
						value="$(commonDir)/system/include/cmsis/arm_const_structs.h" />
					<simple name="target"
						value="$(sysDir)/$(includeDir)/$(cmsisDir)/arm_const_structs.h" />
					<simple name="replaceable" value="true" />
				</element>
				<element>
					<simple name="source" value="$(commonDir)/system/include/cmsis/arm_math.h" />
					<simple name="target"
						value="$(sysDir)/$(includeDir)/$(cmsisDir)/arm_math.h" />
					<simple name="replaceable" value="true" />
				</element>
				<element>
					<simple name="source" value="$(commonDir)/system/include/cmsis/core_cm0.h" />
					<simple name="target"
						value="$(sysDir)/$(includeDir)/$(cmsisDir)/core_cm0.h" />
					<simple name="replaceable" value="true" />
				</element>
				<element>
					<simple name="source"
						value="$(commonDir)/system/include/cmsis/core_cm0plus.h" />
					<simple name="target"
						value="$(sysDir)/$(includeDir)/$(cmsisDir)/core_cm0plus.h" />
					<simple name="replaceable" value="true" />
				</element>
				<element>
					<simple name="source" value="$(commonDir)/system/include/cmsis/core_cm3.h" />
					<simple name="target"
						value="$(sysDir)/$(includeDir)/$(cmsisDir)/core_cm3.h" />
					<simple name="replaceable" value="true" />
				</element>
				<element>
					<simple name="source"
						value="$(commonDir)/system/include/cmsis/core_cm4_simd.h" />
					<simple name="target"
						value="$(sysDir)/$(includeDir)/$(cmsisDir)/core_cm4_simd.h" />
					<simple name="replaceable" value="true" />
				</element>
				<element>
					<simple name="source" value="$(commonDir)/system/include/cmsis/core_cm4.h" />
					<simple name="target"
						value="$(sysDir)/$(includeDir)/$(cmsisDir)/core_cm4.h" />
					<simple name="replaceable" value="true" />
				</element>
				<element>
					<simple name="source" value="$(commonDir)/system/include/cmsis/core_cmFunc.h" />
					<simple name="target"
						value="$(sysDir)/$(includeDir)/$(cmsisDir)/core_cmFunc.h" />
					<simple name="replaceable" value="true" />
				</element>
				<element>
					<simple name="source"
						value="$(commonDir)/system/include/cmsis/core_cmInstr.h" />
					<simple name="target"
						value="$(sysDir)/$(includeDir)/$(cmsisDir)/core_cmInstr.h" />
					<simple name="replaceable" value="true" />
				</element>
				<element>
					<simple name="source" value="$(commonDir)/system/include/cmsis/core_sc000.h" />
					<simple name="target"
						value="$(sysDir)/$(includeDir)/$(cmsisDir)/core_sc000.h" />
					<simple name="replaceable" value="true" />
				</element>
				<element>
					<simple name="source" value="$(commonDir)/system/include/cmsis/core_sc300.h" />
					<simple name="target"
						value="$(sysDir)/$(includeDir)/$(cmsisDir)/core_sc300.h" />
					<simple name="replaceable" value="true" />
				</element>
			</complex-array>
		</process>
	</if>

	<if condition="1==1">
		<process type="org.eclipse.cdt.core.AddFiles">
			<simple name="projectName" value="$(projectName)" />
			<complex-array name="files">
				<element>
					<simple name="source" value="$(commonDir)/system/include/arm/semihosting.h" />
					<simple name="target" value="$(sysDir)/$(includeDir)/arm/semihosting.h" />
					<simple name="replaceable" value="true" />
				</element>
			</complex-array>
		</process>
	</if>

	<if condition="1==1">
		<process type="org.eclipse.cdt.core.AddFiles">
			<simple name="projectName" value="$(projectName)" />
			<complex-array name="files">
				<element>
					<simple name="source" value="$(commonDir)/system/include/cortexm/ExceptionHandlers.h" />
					<simple name="target" value="$(sysDir)/$(includeDir)/cortexm/ExceptionHandlers.h" />
					<simple name="replaceable" value="true" />
				</element>
			</complex-array>
		</process>
	</if>

	<if condition="1==1">
		<process type="org.eclipse.cdt.core.AddFiles">
			<simple name="projectName" value="$(projectName)" />
			<complex-array name="files">
				<element>
					<simple name="source" value="$(commonDir)/system/src/cortexm/_initialize_hardware.c" />
					<simple name="target" value="$(sysDir)/$(sourceDir)/cortexm/_initialize_hardware.c" />
					<simple name="replaceable" value="true" />
				</element>
				<element>
					<simple name="source" value="$(commonDir)/system/src/cortexm/_reset_hardware.c" />
					<simple name="target" value="$(sysDir)/$(sourceDir)/cortexm/_reset_hardware.c" />
					<simple name="replaceable" value="true" />
				</element>
				<element>
					<simple name="source" value="$(commonDir)/system/src/cortexm/exception_handlers.c" />
					<simple name="target" value="$(sysDir)/$(sourceDir)/cortexm/exception_handlers.c" />
					<simple name="replaceable" value="true" />
				</element>
				<element>
					<simple name="source" value="$(commonDir)/system/src/newlib/_exit.c" />
					<simple name="target" value="$(sysDir)/$(sourceDir)/newlib/_exit.c" />
					<simple name="replaceable" value="true" />
				</element>
				<element>
					<simple name="source" value="$(commonDir)/system/src/newlib/_sbrk.c" />
					<simple name="target" value="$(sysDir)/$(sourceDir)/newlib/_sbrk.c" />
					<simple name="replaceable" value="true" />
				</element>
				<element>
					<simple name="source" value="$(commonDir)/system/src/newlib/_startup.c" />
					<simple name="target" value="$(sysDir)/$(sourceDir)/newlib/_startup.c" />
					<simple name="replaceable" value="true" />
				</element>
				<element>
					<simple name="source" value="$(commonDir)/system/src/newlib/_syscalls.c" />
					<simple name="target" value="$(sysDir)/$(sourceDir)/newlib/_syscalls.c" />
					<simple name="replaceable" value="true" />
				</element>
				<element>
					<simple name="source" value="$(commonDir)/system/src/newlib/_write.c" />
					<simple name="target" value="$(sysDir)/$(sourceDir)/newlib/_write.c" />
					<simple name="replaceable" value="true" />
				</element>
				<element>
					<simple name="source" value="$(commonDir)/system/src/newlib/trace.c" />
					<simple name="target" value="$(sysDir)/$(sourceDir)/newlib/trace.c" />
					<simple name="replaceable" value="true" />
				</element>
				<element>
					<simple name="source" value="$(commonDir)/system/src/newlib/README.txt" />
					<simple name="target" value="$(sysDir)/$(sourceDir)/newlib/README.txt" />
					<simple name="replaceable" value="true" />
				</element>
			</complex-array>
		</process>
	</if>

	<if condition="$(fileExtension)==cpp">
		<process type="org.eclipse.cdt.core.AddFiles">
			<simple name="projectName" value="$(projectName)" />
			<complex-array name="files">
				<element>
					<simple name="source" value="$(commonDir)/system/src/newlib/hacks.cpp" />
					<simple name="target" value="$(sysDir)/$(sourceDir)/newlib/hacks.cpp" />
					<simple name="replaceable" value="true" />
				</element>
			</complex-array>
		</process>
	</if>

	<if condition="1==1">
		<!-- libs/Vendor/DEVICE/include -->

		<process type="org.eclipse.cdt.core.AddFiles">
			<simple name="projectName" value="$(projectName)" />
			<complex-array name="files">
				<element>
					<simple name="source" value="$(commonDir)/libs/Vendor/DEVICE/Include/README_DEVICE.txt" />
					<simple name="target"
						value="$(sysDir)/$(includeDir)/$(cmsisDir)/README_DEVICE.txt" />
					<simple name="replaceable" value="true" />
				</element>
			</complex-array>
		</process>

		<process type="org.eclipse.cdt.core.AddFiles">
			<simple name="projectName" value="$(projectName)" />
			<complex-array name="files">
				<element>
					<simple name="source"
						value="$(commonDir)/libs/Vendor/DEVICE/Include/cmsis_device.h" />
					<simple name="target"
						value="$(sysDir)/$(includeDir)/$(cmsisDir)/cmsis_device.h" />
					<simple name="replaceable" value="true" />
				</element>
				<element>
					<simple name="source"
						value="$(commonDir)/libs/Vendor/DEVICE/Include/DEVICE.h" />
					<simple name="target"
						value="$(sysDir)/$(includeDir)/$(cmsisDir)/$(deviceName).h" />
					<simple name="replaceable" value="true" />
				</element>
				<element>
					<simple name="source"
						value="$(commonDir)/libs/Vendor/DEVICE/Include/system_DEVICE.h" />
					<simple name="target"
						value="$(sysDir)/$(includeDir)/$(cmsisDir)/system_$(deviceName).h" />
					<simple name="replaceable" value="true" />
				</element>
			</complex-array>
		</process>

		<process type="org.eclipse.cdt.core.AddFiles">
			<simple name="projectName" value="$(projectName)" />
			<complex-array name="files">
				<element>
					<simple name="source" value="$(commonDir)/libs/Vendor/DEVICE/Source/README_DEVICE.txt" />
					<simple name="target"
						value="$(sysDir)/$(sourceDir)/$(cmsisDir)/README_DEVICE.txt" />
					<simple name="replaceable" value="true" />
				</element>
			</complex-array>
		</process>

		<process type="org.eclipse.cdt.core.AddFiles">
			<simple name="projectName" value="$(projectName)" />
			<complex-array name="files">
				<element>
					<simple name="source"
						value="$(commonDir)/libs/Vendor/DEVICE/Source/system_DEVICE.c" />
					<simple name="target"
						value="$(sysDir)/$(sourceDir)/$(cmsisDir)/system_$(deviceName).c" />
					<simple name="replaceable" value="true" />
				</element>
				<element>
					<simple name="source"
						value="$(commonDir)/libs/Vendor/DEVICE/Source/vectors_DEVICE.c" />
					<simple name="target"
						value="$(sysDir)/$(sourceDir)/$(cmsisDir)/vectors_$(deviceName).c" />
					<simple name="replaceable" value="true" />
				</element>
			</complex-array>
		</process>
	</if>

	<if condition="1==1">
		<!-- Options -->
		<process type="org.eclipse.cdt.managedbuilder.core.SetMBSStringOptionValue">
			<simple name="projectName" value="$(projectName)" />
			<complex-array name="resourcePaths">
				<element>
					<simple name="id"
						value="ilg.gnuarmeclipse.managedbuild.cross.option.arm.target.family" />
					<simple name="value"
						value="ilg.gnuarmeclipse.managedbuild.cross.option.arm.target.mcpu.cortex-m3" />
					<simple name="path" value="" />
				</element>
			</complex-array>
		</process>

		<process type="org.eclipse.cdt.managedbuilder.core.SetMBSStringOptionValue">
			<simple name="projectName" value="$(projectName)" />
			<complex-array name="resourcePaths">
				<element>
					<simple name="id"
						value="ilg.gnuarmeclipse.managedbuild.cross.option.c.compiler.std" />
					<simple name="value"
						value="ilg.gnuarmeclipse.managedbuild.cross.option.c.compiler.std.gnu11" />
					<simple name="path" value="" />
				</element>
			</complex-array>
		</process>

		<process
			type="org.eclipse.cdt.managedbuilder.core.SetMBSBooleanOptionValue">
			<simple name="projectName" value="$(projectName)" />
			<complex-array name="resourcePaths">
				<element>
					<simple name="id"
						value="ilg.gnuarmeclipse.managedbuild.cross.option.warnings.allwarn" />
					<simple name="value" value="true" />
					<simple name="path" value="" />
				</element>
				<element>
					<simple name="id"
						value="ilg.gnuarmeclipse.managedbuild.cross.option.warnings.extrawarn" />
					<simple name="value" value="true" />
					<simple name="path" value="" />
				</element>
			</complex-array>
		</process>

		<process
			type="ilg.gnuarmeclipse.templates.core.ConditionalSetMBSBooleanOptionValue">
			<simple name="projectName" value="$(projectName)" />
			<simple name="condition" value="$(CMmostWarnings) == true" />
			<complex-array name="resourcePaths">
				<element>
					<simple name="id"
						value="ilg.gnuarmeclipse.managedbuild.cross.option.warnings.unused" />
					<simple name="value" value="true" />
					<simple name="path" value="" />
					<simple name="buildType" value="" />
				</element>
				<element>
					<simple name="id"
						value="ilg.gnuarmeclipse.managedbuild.cross.option.warnings.uninitialized" />
					<simple name="value" value="true" />
					<simple name="path" value="" />
					<simple name="buildType" value="" />
				</element>
				<element>
					<simple name="id"
						value="ilg.gnuarmeclipse.managedbuild.cross.option.warnings.missingdeclaration" />
					<simple name="value" value="true" />
