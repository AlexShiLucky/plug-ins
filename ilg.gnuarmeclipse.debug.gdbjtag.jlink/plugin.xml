   <extension
         point="org.eclipse.ui.actionSets">
      <actionSet
            description="Restart Action Set Description"
            id="ilg.gnuarmeclipse.debug.gdbjtag.jlink.actionSet.restart"
            label="Restart Action Set"
            visible="true">
         <action
               class="ilg.gnuarmeclipse.debug.gdbjtag.jlink.ui.ButtonRestartActionDelegate"
               disabledIcon="icons/dlcl16/restart_co.gif"
               icon="icons/elcl16/restart_co.gif"
               id="ilg.gnuarmeclipse.debug.gdbjtag.jlink.action.restart"
               label="%restartButton.label"
               retarget="false"
               style="push"
               toolbarPath="ilg.gnuarmeclipse.debug.gdbjtag.jlink.toolbar.restart/restartGroup"
               tooltip="%restartButton.tooltip">
         </action>
      </actionSet>
   </extension>

