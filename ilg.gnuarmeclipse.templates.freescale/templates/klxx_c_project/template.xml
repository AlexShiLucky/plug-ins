<?xml version="1.0" encoding="ISO-8859-1"?>
<template type="ProjTempl" version="1.0" supplier="Eclipse.org"
	revision="1.0" author="Liviu Ionescu"
	copyright="Copyright (c) 2014 Liviu Ionescu. All rights reserved. This program and the accompanying materials are made available under the terms of the Eclipse Public License v1.0 which accompanies this distribution, and is available athttp://www.eclipse.org/legal/epl-v10.html"
	id="KLxx_C_Project" label="%KLxx.template.label" description="%KLxx.template.description"
	help="help.html">

	<property-group id="chip" label="%KLxx.group.target.label"
		description="%KLxx.group.target.description" type="PAGES-ONLY" help="help.html">
		<property id="KLchipFamily" label="%KLxx.chipFamily.label"
			description="%KLxx.chipFamily.description" type="select" default="MKL25Z"
			hidden="false" persist="true">
			<item value="MKL25Z" label="MKL25Z" />
		</property>
		<property id="KLflashSize" label="%KLxx.flashSize.label"
			description="%KLxx.flashSize.description" type="input" pattern="[0-9]*"
			default="%KLxx.flashSize.default" hidden="false" persist="true" />
		<property id="KLramSize" label="%KLxx.ramSize.label"
			description="%KLxx.ramSize.description" type="input" pattern="[0-9]*"
			default="%KLxx.ramSize.default" hidden="false" persist="true" />

		<property id="KLxtalValue" label="%KLxx.xtalValue.label"
			description="%KLxx.xtalValue.description" type="input" pattern="[0-9]*"
			default="%KLxx.xtalValue.default" hidden="false" persist="true" />

		<property id="KLsyscalls" label="%KLxx.syscalls.label"
			description="%KLxx.syscalls.description" type="select" default="retarget"
			hidden="false" persist="true">
			<item value="none" label="Standalone (no syscalls)" />
			<item value="retarget" label="Retargetting (libnosys)" />
			<item value="semihosting" label="Semihosting (librdimon)" />
		</property>
	</property-group>

	<property-group id="folders" label="%KLxx.group.folders.label"
		description="%KLxx.group.folders.description" type="PAGES-ONLY" help="help.html">
		<property id="includeDir" label="%KLxx.includeDir.label"
			description="%KLxx.includeDir.description" type="input"
			default="%KLxx.includeDir.default" pattern="[a-zA-Z0-9]+" mandatory="true"
			persist="true" />
		<property id="sourceDir" label="%KLxx.sourceDir.label"
			description="%KLxx.sourceDir.description" type="input"
			default="%KLxx.sourceDir.default" pattern="[a-zA-Z0-9]+" mandatory="true"
			persist="true" />
		<property id="libsDir" label="%KLxx.libsDir.label"
			description="%KLxx.libsDir.description" type="input" default="%KLxx.libsDir.default"
			pattern="[a-zA-Z0-9]+" mandatory="true" persist="true" />
		<property id="cmsisDir" label="%KLxx.cmsisDir.label"
			description="%KLxx.cmsisDir.description" type="input"
			default="%KLxx.cmsisDir.default" pattern="[a-zA-Z0-9]+" mandatory="true"
			persist="true" />
		<property id="commonDir" label="%KLxx.commonDir.label"
			description="%KLxx.commonDir.description" type="input"
			default="%KLxx.commonDir.default" pattern="[a-zA-Z0-9]+" mandatory="true"
			persist="true" />
		<property id="driversDir" label="%KLxx.driversDir.label"
			description="%KLxx.driversDir.description" type="input"
			default="%KLxx.driversDir.default" pattern="[a-zA-Z0-9]+" mandatory="true"
			persist="true" />
		<property id="miscDir" label="%KLxx.miscDir.label"
			description="%KLxx.miscDir.description" type="input" default="%KLxx.miscDir.default"
			pattern="[a-zA-Z0-9]+" mandatory="true" persist="true" />
		<property id="platformsDir" label="%KLxx.platformsDir.label"
			description="%KLxx.platformsDir.description" type="input"
			default="%KLxx.platformsDir.default" pattern="[a-zA-Z0-9]+"
			mandatory="true" persist="true" />
		<property id="ldscriptsDir" label="%KLxx.ldscriptsDir.label"
			description="%KLxx.ldscriptsDir.description" type="input"
			default="%KLxx.ldscriptsDir.default" pattern="[a-zA-Z0-9]+"
			mandatory="true" persist="true" />
	</property-group>


	<process type="org.eclipse.cdt.managedbuilder.core.NewManagedProject">
		<simple name="name" value="$(projectName)" />
		<simple name="artifactExtension" value="elf" />
		<simple name="isCProject" value="true" />
	</process>

	<!-- include -->
	<process type="org.eclipse.cdt.core.CreateFolder">
		<simple name="projectName" value="$(projectName)" />
		<simple name="path" value="$(includeDir)" />
	</process>

	<process
		type="org.eclipse.cdt.managedbuilder.core.AppendToMBSStringListOptionValues">
		<simple name="projectName" value="$(projectName)" />
		<complex-array name="resourcePaths">
			<element>
				<simple name="id"
					value="ilg.gnuarmeclipse.managedbuild.cross.option.*.include.paths" />
				<simple-array name="values">
					<element value="&quot;../$(includeDir)&quot;" />
				</simple-array>
				<simple name="path" value="" />
			</element>
		</complex-array>
	</process>

	<process type="org.eclipse.cdt.core.AddFiles">
		<simple name="projectName" value="$(projectName)" />
		<complex-array name="files">
			<element>
				<simple name="source" value="src/common.h" />
				<simple name="target" value="$(includeDir)/common.h" />
				<simple name="replaceable" value="true" />
			</element>
		</complex-array>
	</process>

	<!-- src -->
	<process type="org.eclipse.cdt.core.CreateSourceFolder">
		<simple name="projectName" value="$(projectName)" />
		<simple name="path" value="$(sourceDir)" />
	</process>

	<if condition="$(KLsyscalls)==retarget">
		<process type="org.eclipse.cdt.core.AddFiles">
			<simple name="projectName" value="$(projectName)" />
			<complex-array name="files">
				<element>
					<simple name="source" value="src/main-retarget-1.txt" />
					<simple name="target" value="$(sourceDir)/main.c" />
					<simple name="replaceable" value="true" />
				</element>
			</complex-array>
		</process>
		<process type="org.eclipse.cdt.core.AppendCreate">
			<simple name="projectName" value="$(projectName)" />
			<complex-array name="files">
				<element>
					<simple name="source" value="src/main-comment.txt" />
					<simple name="target" value="$(sourceDir)/main.c" />
					<simple name="replaceable" value="true" />
				</element>
				<element>
					<simple name="source" value="src/main-retarget-2.txt" />
					<simple name="target" value="$(sourceDir)/main.c" />
					<simple name="replaceable" value="true" />
				</element>
				<element>
					<simple name="source" value="src/main-defs.txt" />
					<simple name="target" value="$(sourceDir)/main.c" />
					<simple name="replaceable" value="true" />
				</element>
				<element>
					<simple name="source" value="src/main-hello.txt" />
					<simple name="target" value="$(sourceDir)/main.c" />
					<simple name="replaceable" value="true" />
				</element>
				<element>
					<simple name="source" value="src/main-loop.txt" />
					<simple name="target" value="$(sourceDir)/main.c" />
					<simple name="replaceable" value="true" />
				</element>
				<element>
					<simple name="source" value="src/main-print.txt" />
					<simple name="target" value="$(sourceDir)/main.c" />
					<simple name="replaceable" value="true" />
				</element>
				<element>
					<simple name="source" value="src/main-end.txt" />
					<simple name="target" value="$(sourceDir)/main.c" />
					<simple name="replaceable" value="true" />
				</element>
			</complex-array>
		</process>
	</if>
	<if condition="$(KLsyscalls)==semihosting">
		<process type="org.eclipse.cdt.core.AddFiles">
			<simple name="projectName" value="$(projectName)" />
			<complex-array name="files">
				<element>
					<simple name="source" value="src/main-semihosting-1.txt" />
					<simple name="target" value="$(sourceDir)/main.c" />
					<simple name="replaceable" value="true" />
				</element>
			</complex-array>
		</process>
		<process type="org.eclipse.cdt.core.AppendCreate">
			<simple name="projectName" value="$(projectName)" />
			<complex-array name="files">
				<element>
					<simple name="source" value="src/main-comment.txt" />
					<simple name="target" value="$(sourceDir)/main.c" />
					<simple name="replaceable" value="true" />
				</element>
				<element>
					<simple name="source" value="src/main-semihosting-2.txt" />
					<simple name="target" value="$(sourceDir)/main.c" />
					<simple name="replaceable" value="true" />
				</element>
				<element>
					<simple name="source" value="src/main-defs.txt" />
					<simple name="target" value="$(sourceDir)/main.c" />
					<simple name="replaceable" value="true" />
				</element>
				<element>
					<simple name="source" value="src/main-hello.txt" />
					<simple name="target" value="$(sourceDir)/main.c" />
					<simple name="replaceable" value="true" />
				</element>
				<element>
					<simple name="source" value="src/main-loop.txt" />
					<simple name="target" value="$(sourceDir)/main.c" />
					<simple name="replaceable" value="true" />
				</element>
				<element>
					<simple name="source" value="src/main-print.txt" />
					<simple name="target" value="$(sourceDir)/main.c" />
					<simple name="replaceable" value="true" />
				</element>
				<element>
					<simple name="source" value="src/main-end.txt" />
					<simple name="target" value="$(sourceDir)/main.c" />
					<simple name="replaceable" value="true" />
				</element>
			</complex-array>
		</process>
	</if>
	<if condition="$(KLsyscalls)==none">
		<process type="org.eclipse.cdt.core.AddFiles">
			<simple name="projectName" value="$(projectName)" />
			<complex-array name="files">
				<element>
					<simple name="source" value="src/main-standalone-1.txt" />
					<simple name="target" value="$(sourceDir)/main.c" />
					<simple name="replaceable" value="true" />
				</element>
			</complex-array>
		</process>
		<process type="org.eclipse.cdt.core.AppendCreate">
			<simple name="projectName" value="$(projectName)" />
			<complex-array name="files">
				<element>
					<simple name="source" value="src/main-comment.txt" />
					<simple name="target" value="$(sourceDir)/main.c" />
					<simple name="replaceable" value="true" />
				</element>
				<element>
					<simple name="source" value="src/main-standalone-2.txt" />
					<simple name="target" value="$(sourceDir)/main.c" />
					<simple name="replaceable" value="true" />
				</element>
				<element>
					<simple name="source" value="src/main-defs.txt" />
					<simple name="target" value="$(sourceDir)/main.c" />
					<simple name="replaceable" value="true" />
				</element>
				<element>
					<simple name="source" value="src/main-loop.txt" />
					<simple name="target" value="$(sourceDir)/main.c" />
					<simple name="replaceable" value="true" />
				</element>
				<element>
					<simple name="source" value="src/main-end.txt" />
					<simple name="target" value="$(sourceDir)/main.c" />
					<simple name="replaceable" value="true" />
				</element>
			</complex-array>
		</process>
	</if>

	<!-- ldscripts -->
	<process type="org.eclipse.cdt.core.CreateFolder">
		<simple name="projectName" value="$(projectName)" />
		<simple name="path" value="$(ldscriptsDir)" />
	</process>

	<process type="org.eclipse.cdt.core.AddFiles">
		<simple name="projectName" value="$(projectName)" />
		<complex-array name="files">
			<element>
				<simple name="source" value="ldscripts/mem.ld" />
				<simple name="target" value="$(ldscriptsDir)/mem.ld" />
				<simple name="replaceable" value="true" />
			</element>
			<element>
				<simple name="source" value="../ldscripts/sections.ld" />
				<simple name="target" value="$(ldscriptsDir)/sections.ld" />
				<simple name="replaceable" value="true" />
			</element>
		</complex-array>
	</process>

	<if condition="$(KLsyscalls)==retarget">
		<process type="org.eclipse.cdt.core.AddFiles">
			<simple name="projectName" value="$(projectName)" />
			<complex-array name="files">
				<element>
					<simple name="source" value="../ldscripts/libs-nosys.ld" />
					<simple name="target" value="$(ldscriptsDir)/libs.ld" />
					<simple name="replaceable" value="true" />
				</element>
			</complex-array>
		</process>
	</if>
	<if condition="$(KLsyscalls)==semihosting">
		<process type="org.eclipse.cdt.core.AddFiles">
			<simple name="projectName" value="$(projectName)" />
			<complex-array name="files">
				<element>
					<simple name="source" value="../ldscripts/libs-rdimon.ld" />
					<simple name="target" value="$(ldscriptsDir)/libs.ld" />
					<simple name="replaceable" value="true" />
				</element>
			</complex-array>
		</process>
	</if>
	<if condition="$(KLsyscalls)==none">
		<process type="org.eclipse.cdt.core.AddFiles">
			<simple name="projectName" value="$(projectName)" />
			<complex-array name="files">
				<element>
					<simple name="source" value="../ldscripts/libs.ld" />
					<simple name="target" value="$(ldscriptsDir)/libs.ld" />
					<simple name="replaceable" value="true" />
				</element>
			</complex-array>
		</process>
	</if>

	<!-- libs as src -->
	<process type="org.eclipse.cdt.core.CreateSourceFolder">
		<simple name="projectName" value="$(projectName)" />
		<simple name="path" value="$(libsDir)" />
	</process>

	<!-- CMSIS -->
	<process type="org.eclipse.cdt.core.CreateFolder">
		<simple name="projectName" value="$(projectName)" />
		<simple name="path" value="$(libsDir)/$(cmsisDir)/$(includeDir)" />
	</process>

	<process type="org.eclipse.cdt.core.CreateFolder">
		<simple name="projectName" value="$(projectName)" />
		<simple name="path" value="$(libsDir)/$(cmsisDir)/$(sourceDir)" />
	</process>

	<process
		type="org.eclipse.cdt.managedbuilder.core.AppendToMBSStringListOptionValues">
		<simple name="projectName" value="$(projectName)" />
		<complex-array name="resourcePaths">
			<element>
				<simple name="id"
					value="ilg.gnuarmeclipse.managedbuild.cross.option.*.include.paths" />
				<simple-array name="values">
					<element value="&quot;../$(libsDir)/$(cmsisDir)/$(includeDir)&quot;" />
				</simple-array>
				<simple name="path" value="" />
			</element>
		</complex-array>
	</process>

	<process type="org.eclipse.cdt.core.AddFiles">
		<simple name="projectName" value="$(projectName)" />
		<complex-array name="files">
			<element>
				<simple name="source" value="../originals/CMSIS/Include/core_cm0plus.h" />
				<simple name="target"
					value="$(libsDir)/$(cmsisDir)/$(includeDir)/core_cm0plus.h" />
				<simple name="replaceable" value="true" />
			</element>
			<element>
				<simple name="source" value="../originals/CMSIS/Include/core_cmFunc.h" />
				<simple name="target"
					value="$(libsDir)/$(cmsisDir)/$(includeDir)/core_cmFunc.h" />
				<simple name="replaceable" value="true" />
			</element>
			<element>
				<simple name="source" value="../originals/CMSIS/Include/core_cmInstr.h" />
				<simple name="target"
					value="$(libsDir)/$(cmsisDir)/$(includeDir)/core_cmInstr.h" />
				<simple name="replaceable" value="true" />
			</element>
		</complex-array>
	</process>

	<if condition="$(KLchipFamily)==MKL25Z">
		<process type="org.eclipse.cdt.core.AddFiles">
			<simple name="projectName" value="$(projectName)" />
			<complex-array name="files">
				<element>
					<simple name="source"
						value="../originals/klxx-sc-baremetal/src/cpu/headers/CMSIS/MemMapPtr_KL25Z4.h" />
					<simple name="target"
						value="$(libsDir)/$(cmsisDir)/$(includeDir)/MemMapPtr_KL25Z4.h" />
					<simple name="replaceable" value="true" />
				</element>
				<element>
					<simple name="source"
						value="../originals/klxx-sc-baremetal/src/cpu/headers/CMSIS/MKL25Z4/Include/MKL25Z4.h" />
					<simple name="target"
						value="$(libsDir)/$(cmsisDir)/$(includeDir)/MKL25Z4.h" />
					<simple name="replaceable" value="true" />
				</element>
				<element>
					<simple name="source"
						value="../originals/klxx-sc-baremetal/src/cpu/headers/CMSIS/MKL25Z4/Include/system_MKL25Z4.h" />
					<simple name="target"
						value="$(libsDir)/$(cmsisDir)/$(includeDir)/system_MKL25Z4.h" />
					<simple name="replaceable" value="true" />
				</element>
				<element>
					<simple name="source"
						value="../originals/klxx-sc-baremetal/src/cpu/vectors.h" />
					<simple name="target"
						value="$(libsDir)/$(cmsisDir)/$(includeDir)/vectors.h" />
					<simple name="replaceable" value="true" />
				</element>
			</complex-array>
		</process>

		<process type="org.eclipse.cdt.core.AddFiles">
			<simple name="projectName" value="$(projectName)" />
			<complex-array name="files">
				<element>
					<simple name="source" value="../common/src/startup_cm.c" />
					<simple name="target"
						value="$(libsDir)/$(cmsisDir)/$(sourceDir)/startup_cm.c" />
					<simple name="replaceable" value="true" />
				</element>
				<element>
					<simple name="source" value="src/startup_MKL25Z4.c" />
					<simple name="target"
						value="$(libsDir)/$(cmsisDir)/$(sourceDir)/startup_MKL25Z4.c" />
					<simple name="replaceable" value="true" />
				</element>
				<element>
					<simple name="source"
						value="src/system_MKL25Z4.c.patched" />
					<simple name="target"
						value="$(libsDir)/$(cmsisDir)/$(sourceDir)/system_MKL25Z4.c" />
					<simple name="replaceable" value="true" />
				</element>
				<element>
					<simple name="source" value="src/vectors_MKL25Z4.c" />
					<simple name="target"
						value="$(libsDir)/$(cmsisDir)/$(sourceDir)/vectors_MKL25Z4.c" />
					<simple name="replaceable" value="true" />
				</element>
			</complex-array>
		</process>
	</if>

	<!-- common -->
	<process type="org.eclipse.cdt.core.CreateFolder">
		<simple name="projectName" value="$(projectName)" />
		<simple name="path" value="$(libsDir)/$(commonDir)/$(includeDir)" />
	</process>

	<process type="org.eclipse.cdt.core.CreateFolder">
		<simple name="projectName" value="$(projectName)" />
		<simple name="path" value="$(libsDir)/$(commonDir)/$(sourceDir)" />
	</process>

	<process
		type="org.eclipse.cdt.managedbuilder.core.AppendToMBSStringListOptionValues">
		<simple name="projectName" value="$(projectName)" />
		<complex-array name="resourcePaths">
			<element>
				<simple name="id"
					value="ilg.gnuarmeclipse.managedbuild.cross.option.*.include.paths" />
				<simple-array name="values">
					<element value="&quot;../$(libsDir)/$(commonDir)/$(includeDir)&quot;" />
				</simple-array>
				<simple name="path" value="" />
			</element>
		</complex-array>
	</process>

	<process type="org.eclipse.cdt.core.AddFiles">
		<simple name="projectName" value="$(projectName)" />
		<complex-array name="files">
			<element>
				<simple name="source"
					value="../originals/klxx-sc-baremetal/src/common/assert.h" />
				<simple name="target" value="$(libsDir)/$(commonDir)/$(includeDir)/assert.h" />
				<simple name="replaceable" value="true" />
			</element>
			<element>
				<simple name="source" value="../originals/klxx-sc-baremetal/src/common/io.h" />
				<simple name="target" value="$(libsDir)/$(commonDir)/$(includeDir)/io.h" />
				<simple name="replaceable" value="true" />
			</element>
			<element>
				<simple name="source"
					value="../originals/klxx-sc-baremetal/src/common/memtest.h" />
				<simple name="target"
					value="$(libsDir)/$(commonDir)/$(includeDir)/memtest.h" />
				<simple name="replaceable" value="true" />
			</element>
			<element>
				<simple name="source"
					value="../originals/klxx-sc-baremetal/src/common/queue.h" />
				<simple name="target" value="$(libsDir)/$(commonDir)/$(includeDir)/queue.h" />
				<simple name="replaceable" value="true" />
			</element>
			<element>
				<simple name="source"
					value="../originals/klxx-sc-baremetal/src/common/startup.h" />
				<simple name="target"
					value="$(libsDir)/$(commonDir)/$(includeDir)/startup.h" />
				<simple name="replaceable" value="true" />
			</element>
			<element>
				<simple name="source"
					value="../originals/klxx-sc-baremetal/src/common/stdlib.h" />
				<simple name="target" value="$(libsDir)/$(commonDir)/$(includeDir)/stdlib.h" />
				<simple name="replaceable" value="true" />
			</element>
			<element>
				<simple name="source"
					value="../originals/klxx-sc-baremetal/src/common/typedef.h" />
				<simple name="target"
					value="$(libsDir)/$(commonDir)/$(includeDir)/typedef.h" />
				<simple name="replaceable" value="true" />
			</element>
			<element>
				<simple name="source"
					value="../originals/klxx-sc-baremetal/src/common/uif.h" />
				<simple name="target" value="$(libsDir)/$(commonDir)/$(includeDir)/uif.h" />
				<simple name="replaceable" value="true" />
			</element>
		</complex-array>
	</process>

	<process type="org.eclipse.cdt.core.AddFiles">
		<simple name="projectName" value="$(projectName)" />
		<complex-array name="files">
			<element>
				<simple name="source" value="../common/src/alloc.c.patched" />
				<simple name="target" value="$(libsDir)/$(commonDir)/$(sourceDir)/alloc.c_" />
				<simple name="replaceable" value="true" />
			</element>
			<element>
				<simple name="source"
					value="../originals/klxx-sc-baremetal/src/common/assert.c" />
				<simple name="target" value="$(libsDir)/$(commonDir)/$(sourceDir)/assert.c_" />
				<simple name="replaceable" value="true" />
			</element>
			<element>
				<simple name="source" value="../originals/klxx-sc-baremetal/src/common/io.c" />
				<simple name="target" value="$(libsDir)/$(commonDir)/$(sourceDir)/io.c_" />
				<simple name="replaceable" value="true" />
			</element>
			<element>
				<simple name="source"
					value="../originals/klxx-sc-baremetal/src/common/printf.c" />
				<simple name="target" value="$(libsDir)/$(commonDir)/$(sourceDir)/printf.c_" />
				<simple name="replaceable" value="true" />
			</element>
			<element>
				<simple name="source"
