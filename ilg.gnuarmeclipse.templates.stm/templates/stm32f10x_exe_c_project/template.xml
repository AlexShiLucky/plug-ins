	id="stm32f10x.exe.c.project" label="%STM32F10x.Ctemplate.label"
	description="%STM32F10x.Ctemplate.description" help="help.html">

	<property-group id="chip" label="%STM32F10x.group.target.label"
		description="%STM32F10x.group.target.description" type="PAGES-ONLY"
		help="help.html">
		<property id="STM32F1chipFamily" label="%STM32F10x.chipFamily.label"
			description="%STM32F10x.chipFamily.description" type="select"
			default="%STM32F10x.chipFamily.default" hidden="false" persist="true">
			<item value="STM32F10X_LD" label="STM32f10x Low Density" />
			<item value="STM32F10X_LD_VL" label="STM32f10x Low Density Value Line" />
			<item value="STM32F10X_MD" label="STM32f10x Medium Density" />
			<item value="STM32F10X_MD_VL" label="STM32f10x Medium Density Value Line" />
			<item value="STM32F10X_HD" label="STM32f10x High Density" />
			<item value="STM32F10X_HD_VL" label="STM32f10x High Density Value Line" />
			<item value="STM32F10X_CL" label="STM32f10x Connectivity line" />
			<item value="STM32F10X_XL" label="STM32f10x Extra Large Density" />
		</property>
		<property id="STM32F1flashSize" label="%STM32F10x.flashSize.label"
			description="%STM32F10x.flashSize.description" type="input" pattern="[0-9]*"
			default="%STM32F10x.flashSize.default" hidden="false" persist="true" />
		<property id="STM32F1ramSize" label="%STM32F10x.ramSize.label"
			description="%STM32F10x.ramSize.description" type="input" pattern="[0-9]*"
			default="%STM32F10x.ramSize.default" hidden="false" persist="true" />

		<property id="STM32F1syscalls" label="%STM32F10x.syscalls.label"
			description="%STM32F10x.syscalls.description" type="select" default="retarget"
			hidden="false" persist="true">
			<item value="retarget" label="Retargetting (libnosys)" />
			<item value="semihosting" label="Semihosting (librdimon)" />
			<item value="none" label="Standalone (no syscalls)" />
		</property>

		<!-- <property id="includeUsb" label="%STM32F10x.includeUsb.label" description="%STM32F10x.includeUsb.description" 
			type="select" default="%STM32F10x.includeUsb.default" hidden="false" persist="true"> 
			<item value="none" label="None" /> <item value="device" label="Device" /> 
			<item value="host" label="Host" /> <item value="otg" label="OTG" /> </property> -->
