	id="stm32f4xx.exe.c.project" label="%STM32F4xx.Ctemplate.label"
	description="%STM32F4xx.Ctemplate.description" help="help.html">

	<property-group id="chip" label="%STM32F4xx.group.target.label"
		description="%STM32F4xx.group.target.description" type="PAGES-ONLY"
		help="help.html">
		<property id="STM32F4chipFamily" label="%STM32F4xx.chipFamily.label"
			description="%STM32F4xx.chipFamily.description" type="select"
			default="%STM32F4xx.chipFamily.default" hidden="false" persist="true">
			<item value="STM32F401xx" label="STM32F401xx" />
			<item value="STM32F40_41xxx" label="STM32F40_41xxx" />
			<item value="STM32F427_437xx" label="STM32F427_437xx" />
			<item value="STM32F429_439xx" label="STM32F429_439xx" />
		</property>
		<property id="STM32F4flashSize" label="%STM32F4xx.flashSize.label"
			description="%STM32F4xx.flashSize.description" type="input" pattern="[0-9]*"
			default="%STM32F4xx.flashSize.default" hidden="false" persist="true" />

		<property id="STM32F4hseValue" label="%STM32F4xx.hseValue.label"
			description="%STM32F4xx.hseValue.description" type="input" pattern="[0-9]*"
			default="%STM32F4xx.hseValue.default" hidden="false" persist="true" />

		<property id="STM32F4syscalls" label="%STM32F4xx.syscalls.label"
			description="%STM32F4xx.syscalls.description" type="select" default="retarget"
			hidden="false" persist="true">
			<item value="none" label="Standalone (no syscalls)" />
			<item value="retarget" label="Retargetting (libnosys)" />
			<item value="semihosting" label="Semihosting (librdimon)" />
		</property>

